  Owner                    = {patta},
  Pii                      = {S0006-3495(00)76607-0},
  Pmid                     = {10620308},
  Timestamp                = {2012.05.25},
  Url                      = {http://dx.doi.org/10.1016/S0006-3495(00)76607-0}
}

