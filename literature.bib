}

@Article{Chien1993,
  Title                    = {{N}avigational errors made by growth cones without filopodia in the embryonic {X}enopus brain.},
  Author                   = {Chien, C. B. and Rosenthal, D. E. and Harris, W. A. and Holt, C. E.},
