@Article{Sondell2001,
  Title                    = {{P}ostnatal expression of {VEGF} and its receptor flk-1 in peripheral ganglia.},
  Author                   = {Sondell, M. and Kanje, M.},
  Journal                  = {Neuroreport},
  Year                     = {2001},

  Month                    = {Jan},
  Number                   = {1},
  Pages                    = {105--108},
  Volume                   = {12},

  Abstract                 = {The postnatal (P0-P12) and adult expression of vascular endothelial growth factor and its receptor flk-1 was investigated in superior cervical (SCG) and dorsal root ganglia (DRG) in mice by immunocytochemistry. At P0 all neurons in SCG and DRG contained VEGF. The number of VEGF-immunoreactive neurons in DRG but not in SCG, decreased postnatally and reached adult levels (34\%) at P12. At P0 flk-1 was found in virtually all neurons in the SCG and in roughly half of the neurons in DRG. The number of flk-1 positive neurons then decreased and reached adult levels at P12. The findings demonstrate temporal changes in VEGF and flk-1 expression, suggesting developmental regulation of VEGF activity in peripheral ganglia.},
