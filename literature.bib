@Article{Lee1999,
  Title                    = {{R}egulation of cell movement is mediated by stretch-activated calcium channels.},
  Author                   = {Lee, J. and Ishihara, A. and Oxford, G. and Johnson, B. and Jacobson, K.},
  Journal                  = {Nature},
