@Article{Kolodkin1997a,
  Title                    = {{N}europilin is a semaphorin {III} receptor.},
  Author                   = {Kolodkin, A. L. and Levengood, D. V. and Rowe, E. G. and Tai, Y. T. and Giger, R. J. and Ginty, D. D.},
