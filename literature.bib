@Article{Sasaki1992,
  Title                    = {{R}uthenium red inhibits the binding of calcium to calmodulin required for enzyme activation.},
  Author                   = {Sasaki, T. and Naka, M. and Nakamura, F. and Tanaka, T.},
  Journal                  = {J Biol Chem},
