@Article{Tsai2005,
  Title                    = {{N}ucleokinesis in neuronal migration.},
  Author                   = {Tsai, Li-Huei and Gleeson, Joseph G.},
  Journal                  = {Neuron},
  Year                     = {2005},

  Month                    = {May},
  Number                   = {3},
  Pages                    = {383--388},
  Volume                   = {46},

  Abstract                 = {Neuronal migration is a critical phase of nervous system development and can be divided into two distinct phases: extension of the leading process and movement of the cell body and nucleus (nucleokinesis). Nucleokinesis appears to require many of the same cytoskeletal and signaling molecules used in cell mitosis. Converging studies suggest it requires cytoplasmic dynein, cell polarity genes, and microtubule-associated proteins that coordinate microtubule remodeling. These coordinate first the positioning of the centrosome (microtubule organizing center) in the leading process in front of the nucleus and then the movement of the nucleus towards the centrosome. The positioning of the centrosome and the dynamic regulation that couples and uncouples the nucleus underlies directed migration of neurons.},
  Doi                      = {10.1016/j.neuron.2005.04.013},
  Institution              = {Department of Pathology, Harvard Medical School, Howard Hughes Medical Institute, 77 Avenue Louis Pasteur, Room 858C, Boston, Massachusetts 02115, USA. li-huei_tsai@hms.harvard.edu},
  Keywords                 = {Animals; Brain, cytology/embryology; Cell Movement, physiology; Cell Nucleus, physiology; Humans; Microtubules, metabolism; Models, Neurological; Neurons, cytology/physiology},
  Language                 = {eng},
  Medline-pst              = {ppublish},
  Owner                    = {patta},
  Pii                      = {S0896-6273(05)00349-1},
  Pmid                     = {15882636},
  Timestamp                = {2015.05.31},
  Url                      = {http://dx.doi.org/10.1016/j.neuron.2005.04.013}
}

@Article{Tsurui2000,
  Title                    = {{S}even-color fluorescence imaging of tissue samples based on {F}ourier spectroscopy and singular value decomposition.},
  Author                   = {Tsurui, H. and Nishimura, H. and Hattori, S. and Hirose, S. and Okumura, K. and Shirai, T.},
  Journal                  = {J Histochem Cytochem},
  Year                     = {2000},

  Month                    = {May},
  Number                   = {5},
  Pages                    = {653--662},
  Volume                   = {48},

  Abstract                 = {Seven-color analyses of immunofluorescence-stained tissue samples were accomplished using Fourier spectroscopy-based hyperspectral imaging and singular value decomposition. This system consists of a combination of seven fluorescent dyes, three filtersets, an epifluorescence microscope, a spectral imaging system, a computer for data acquisition, and data analysis software. The spectra of all pixels in a multicolor image were taken simultaneously using a Sagnac type interferometer. The spectra were deconvolved to estimate the contribution of each component dye, and individual dye images were constructed based on the intensities of assigned signals. To obtain mixed spectra, three filter sets, i.e., Bl, Gr, and Rd for Alexa488 and Alexa532, for Alexa546, Alexa568, and Alexa594, and for Cy5 and Cy5.5, respectively, were used for simultaneous excitation of two or three dyes. These fluorophores have considerable spectral overlap which precludes their separation by conventional analysis. We resolved their relative contributions to the fluorescent signal by a method involving linear unmixing based on singular value decomposition of the matrices consisting of dye spectra. Analyses of mouse thymic tissues stained with seven different fluorescent dyes provided clear independent images, and any combination of two or three individual dye images could be used for constructing multicolor images.},
  Institution              = {Department of Pathology, Juntendo University School of Medicine, Tokyo, Japan. tsurui@med.juntendo.ac.jp},
  Keywords                 = {Animals; Antigens, CD, analysis/metabolism; Color; Fluorescent Dyes, chemistry; Fourier Analysis; Image Enhancement, instrumentation/methods; Mice; Mice, Inbred BALB C; Mice, Inbred NZB; Microscopy, Fluorescence, instrumentation/methods; Microscopy, Interference; Optics and Photonics; Receptors, Antigen, T-Cell, alpha-beta, analysis/metabolism; Signal Processing, Computer-Assisted; Spectrum Analysis, instrumentation; Spleen, chemistry/cytology/metabolism; Thymus Gland, chemistry/cytology/metabolism},
  Language                 = {eng},
