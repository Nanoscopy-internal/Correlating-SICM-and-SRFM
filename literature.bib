@Article{Loitto2002,
  Title                    = {{N}eutrophil leukocyte motility requires directed water influx.},
  Author                   = {Loitto, Vesa-Matti and Forslund, Tony and Sundqvist, Tommy and Magnusson, Karl-Eric and Gustafsson, Mikael},
  Journal                  = {J Leukoc Biol},
