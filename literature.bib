@Article{Nakazawa2012,
  Title                    = {{R}ab33a mediates anterograde vesicular transport for membrane exocytosis and axon outgrowth.},
  Author                   = {Nakazawa, Hitomi and Sada, Tadayuki and Toriyama, Michinori and Tago, Kenji and Sugiura, Tadao and Fukuda, Mitsunori and Inagaki, Naoyuki},
  Journal                  = {J Neurosci},
  Year                     = {2012},

