@Article{Munevar2004,
  Title                    = {{R}egulation of mechanical interactions between fibroblasts and the substratum by stretch-activated {C}a2+ entry.},
  Author                   = {Munevar, Steven and Wang, Yu-Li and Dembo, Micah},
  Journal                  = {J Cell Sci},
