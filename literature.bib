@Article{Jin2003,
  Title                    = {{H}epatocyte growth factor/scatter factor stimulates {C}a2+-activated membrane {K}+ current and migration of {MDCK} {II} cells.},
  Author                   = {Jin, M. and Defoe, D. M. and Wondergem, R.},
  Journal                  = {J Membr Biol},
  Year                     = {2003},

  Month                    = {Jan},
  Number                   = {1},
  Pages                    = {77--86},
  Volume                   = {191},

  Abstract                 = {Hepatocyte growth factor/scatter factor (HGF/SF) stimulates migration of various cells and has been linked via Met tyrosine kinase-signaling to transformation and the metastatic phenotype. Migration of transformed MDCK-F cells depends on activation of a charybdotoxin-sensitive, volume-activated membrane K+ current. Thus, we used patch-clamp electrophysiology and transwell migration assays to determine whether HGF/SF stimulation of MDCK II cell migration depends on the activation of membrane K+ currents. HGF/SF activated a membrane K+ current that increased over 24 hr, and which could be modulated by increasing intracellular calcium concentration, [Ca2+]i. Charybdotoxin (ChTX, 50 nM), iberiotoxin (IbTX, 100 nM), stichodactyla toxin (Stk, 100 nM) and clotrimazole (CLT, 1 mM) all inhibited this current. HGF/SF (100 scatter units/ml) significantly increased MDCK II cell migration over 8 hr compared to control cells. Addition of ChTX (50 nM), IbTX (100 nM), Stk (100 nM) or CLT (1 microM) inhibited the HGF/SF-stimulated MDCK II cell migration. We conclude that the activation of membrane Ca2+-activated K+current is necessary for HGF/SF stimulation of MDCK II cell.},
  Doi                      = {10.1007/s00232-002-1045-1},
  Institution              = {Department of Physiology, James H. Quillen College of Medicine, East Tennessee State University, PO Box 70,576, Johnson City, TN 37614, USA.},
  Keywords                 = {Animals; Cell Line; Cell Movement, drug effects/physiology; Charybdotoxin, pharmacology; Clotrimazole, pharmacology; Dogs; Epithelium, drug effects/physiology/ultrastructure; Hepatocyte Growth Factor, pharmacology; Ion Channel Gating, drug effects/physiology; Kidney, drug effects/physiology/ultrastructure; Marine Toxins, pharmacology; Membrane Potentials, drug effects/physiology; Peptides, pharmacology; Potassium Channels, Calcium-Activated, drug effects/physiology},
