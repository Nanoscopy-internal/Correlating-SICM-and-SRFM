@Article{Korchev2004,
  Title                    = {{P}olymer-initiated photogeneration of silver nanoparticles in {SPEEK}/{PVA} films: direct metal photopatterning.},
  Author                   = {Korchev, A. S. and Bozack, M. J. and Slaten, B. L. and Mills, G.},
