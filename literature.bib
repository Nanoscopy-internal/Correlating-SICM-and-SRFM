  Timestamp                = {2012.05.25},
  Url                      = {http://dx.doi.org/10.1021/ja8022253}
}

@Article{Potter2011,
  Title                    = {{R}ole of shear stress in endothelial cell morphology and expression of cyclooxygenase isoforms.},
  Author                   = {Potter, Claire M F. and Lundberg, Martina H. and Harrington, Louise S. and Warboys, Christina M. and Warner, Timothy D. and Berson, R Eric and Moshkov, Alexey V. and Gorelik, Julia and Weinberg, Peter D. and Mitchell, Jane A.},
  Journal                  = {Arterioscler Thromb Vasc Biol},
  Year                     = {2011},

