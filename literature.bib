@Article{Marko2008,
  Title                    = {{VEGF} promotes vascular sympathetic innervation.},
  Author                   = {Marko, Stephen B. and Damon, Deborah H.},
  Journal                  = {Am J Physiol Heart Circ Physiol},
  Year                     = {2008},

  Month                    = {Jun},
  Number                   = {6},
  Pages                    = {H2646--H2652},
  Volume                   = {294},

  Abstract                 = {The sympathetic nervous system, via postganglionic innervation of blood vessels and the heart, is an important determinant of cardiovascular function. The mechanisms underlying sympathetic innervation of targets are not fully understood. This study tests the hypothesis that target-derived vascular endothelial growth factor (VEGF) promotes sympathetic innervation of blood vessels. Western blot and immunohistochemical analyses indicate that VEGF is produced by vascular cells in arteries and that VEGF receptors are expressed on sympathetic nerve fibers innervating arteries. In vitro, exogenously added VEGF and VEGF produced by vascular smooth muscle cells (VSMCs) in sympathetic neurovascular cocultures inhibited semaphorin 3A (Sema3A)-induced collapse of sympathetic growth cones. In the absence of Sema3A, VEGF and VSMCs also increased growth cone area. These effects were mediated via VEGF receptor 1. In vivo, the neutralization of VEGF inhibited the reinnervation of denervated femoral arteries. These data demonstrate that target-derived VEGF plays a previously unrecognized role in promoting the growth of sympathetic axons.},
  Doi                      = {10.1152/ajpheart.00291.2008},
  Institution              = {Department of Pharmacology, University of Vermont, Burlington, Vermont, USA.},
  Keywords                 = {Animals; Animals, Newborn; Blotting, Western; Cells, Cultured; Coculture Techniques; Femoral Artery, innervation/metabolism; Growth Cones, metabolism; Immunohistochemistry; Muscle, Smooth, Vascular, innervation/metabolism; Myocytes, Smooth Muscle, metabolism; Neurons, metabolism; Neuropilin-1, metabolism; Paracrine Communication; Rats; Rats, Sprague-Dawley; Recombinant Proteins, metabolism; Semaphorin-3A, metabolism; Superior Cervical Ganglion, metabolism; Sympathectomy; Sympathetic Fibers, Postganglionic, metabolism/surgery; Time Factors; Vascular Endothelial Growth Factor A, metabolism; Vascular Endothelial Growth Factor Receptor-1, metabolism; Vascular Endothelial Growth Factor Receptor-2, metabolism},
