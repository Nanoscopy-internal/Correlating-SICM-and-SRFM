@Article{Piper2008,
  Title                    = {{C}haracterization and application of controllable local chemical changes produced by reagent delivery from a nanopipet.},
  Author                   = {Piper, Joe D. and Li, Chao and Lo, Chien-Jung and Berry, Richard and Korchev, Yuri and Ying, Liming and Klenerman, David},
