  Owner                    = {patta},
  Pmid                     = {12221130},
  Timestamp                = {2016.02.18},
  Url                      = {http://dx.doi.org/10.1091/mbc.E02-04-0231}
}

@Article{VanTroys2008,
  Title                    = {{I}ns and outs of {ADF}/cofilin activity and regulation.},
  Author                   = {{Van Troys}, Marleen and Huyck, Lynn and Leyman, Shirley and Dhaese, Stien and Vandekerkhove, Jo{\"{e}}l and Ampe, Christophe},
