@Article{Schratzberger2000,
  Title                    = {{F}avorable effect of {VEGF} gene transfer on ischemic peripheral neuropathy.},
  Author                   = {Schratzberger, P. and Schratzberger, G. and Silver, M. and Curry, C. and Kearney, M. and Magner, M. and Alroy, J. and Adelman, L. S. and Weinberg, D. H. and Ropper, A. H. and Isner, J. M.},
  Journal                  = {Nat Med},
  Year                     = {2000},

  Month                    = {Apr},
  Number                   = {4},
  Pages                    = {405--413},
  Volume                   = {6},

  Abstract                 = {Ischemic peripheral neuropathy is a frequent, irreversible complication of lower extremity vascular insufficiency. We investigated whether ischemic peripheral neuropathy could be prevented and/or reversed by gene transfer of an endothelial cell mitogen designed to promote therapeutic angiogenesis. Intramuscular gene transfer of naked DNA encoding vascular endothelial growth factor (VEGF) simultaneously with induction of hindlimb ischemia in rabbits abrogated the substantial decrease in motor and sensory nerve parameters, and nerve function recovered promptly. When gene transfer was administered 10 days after induction of ischemia, nerve function was restored earlier and/or recovered faster than in untreated rabbits. These findings are due in part to enhanced hindlimb perfusion. In addition, however, the demonstration of functional VEGF receptor expression by Schwann cells indicates a direct effect of VEGF on neural integrity as well. These findings thus constitute a new paradigm for the treatment of ischemic peripheral neuropathy.},
  Doi                      = {10.1038/74664},
  Institution              = {Division of Cardiovascular Research St. Elizabeth's Medical Center-Tufts University School of Medicine, 736 Cambridge Street, Boston, Massachusetts 02135, USA.},
  Keywords                 = {Animals; Cell Movement, drug effects/physiology; Cell Survival, drug effects; Cells, Cultured; Disease Models, Animal; Endothelial Growth Factors, genetics/pharmacology; Gene Transfer Techniques; Genetic Therapy; Hindlimb, innervation/metabolism/physiopathology; Ischemia, therapy; Lymphokines, genetics/pharmacology; Nerve Tissue Proteins, genetics/metabolism; Neuropilin-1; Peripheral Nervous System Diseases, metabolism/therapy; Peripheral Nervous System, blood supply; Proto-Oncogene Proteins, genetics/metabolism; Rabbits; Rats; Rats, Sprague-Dawley; Receptor Protein-Tyrosine Kinases, genetics/metabolism; Receptors, Growth Factor, genetics/metabolism; Receptors, Vascular Endothelial Growth Factor; Schwann Cells, drug effects/physiology; Vascular Endothelial Growth Factor A; Vascular Endothelial Growth Factor Receptor-1; Vascular Endothelial Growth Factors},
