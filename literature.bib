  Owner                    = {patta},
  Pii                      = {nnano.2013.255},
  Pmid                     = {24317283},
  Timestamp                = {2013.12.11},
  Url                      = {http://dx.doi.org/10.1038/nnano.2013.255}
}

@Article{Vriens2004,
