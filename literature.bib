@Article{Decker2000,
  Title                    = {{O}ligodendrocyte precursor migration and differentiation: combined effects of {PSA} residues, growth factors, and substrates.},
  Author                   = {Decker, L. and Avellana-Adalid, V. and Nait-Oumesmar, B. and Durbec, P. and {Baron-Van Evercooren}, A.},
