}

@Article{Nelson1995,
  Title                    = {{R}elaxation of arterial smooth muscle by calcium sparks.},
  Author                   = {Nelson, M. T. and Cheng, H. and Rubart, M. and Santana, L. F. and Bonev, A. D. and Knot, H. J. and Lederer, W. J.},
