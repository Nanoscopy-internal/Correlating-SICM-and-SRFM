  year        = {1994},
  month       = {Sep},
  number      = {1350},
  pages       = {255--261},
  volume      = {257},
  abstract    = {The data presented here describe the first unequivocable characterization of a pore-forming protein in any helminth parasite. The excretory/secretory (E/S) material of the human whipworm T. trichiura contains a highly abundant protein of molecular mass 47 kDa (TT47); the murine model parasite, T. muris, contains a similarly abundant protein of molecular mass 43 kDa (TM43). When purified to homogeneity, these proteins induce ion-conducting pores in lipid bilayers. Antibodies raised against TM43 abolish channel activity. Pore formation in epithelial cell membranes may facilitate invasion of the host gut by Trichuris and enable the parasite to maintain its syncytial environment in the caecal epithelium. The observation that this activity may be inhibited by antibody opens a possible avenue for drug and vaccine development.},
  doi         = {10.1098/rspb.1994.0123},
  institution = {Medicine, South Kensington, London, U.K.},
  keywords    = {Animals; Antibodies; Cell Membrane, physiology; Electrophoresis, Polyacrylamide Gel; Helminth Proteins, isolation /&/ purification/physiology; Humans; Lipid Bilayers; Membrane Proteins, isolation /&/ purification/physiology; Mice; Molecular Weight; Phosphatidylcholines; Trichuris, chemistry/physiology},
  language    = {eng},
  medline-pst = {ppublish},
  owner       = {patta},
  pmid        = {7991635},
  timestamp   = {2012.05.25},
  url         = {http://dx.doi.org/10.1098/rspb.1994.0123},
}

@Article{Drake1998,
  Title                    = {{M}olecular and functional characterization of a recombinant protein of {T}richuris trichiura.},
